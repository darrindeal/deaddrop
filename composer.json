{
  "name": "darrindeal/deaddrop",
  "description": "",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "role": "Developer",
      "name": "Darrin Deal",
      "email": "darrin.deal@icloud.com",
      "homepage": "https://www.darrindeal.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "DarrinDeal\\DeadDrop\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DarrinDeal\\DeadDrop\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/console": "*",
    "illuminate/support": "^12.17",
    "laravel/prompts": "^0.3.5",
    "symfony/finder": "^7.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require-dev": {
    "laravel/pint": "^1.22",
    "orchestra/testbench": "^10.4",
    "pestphp/pest": "^3.8",
    "phpstan/phpstan": "^2.1"
  }
}
